#include<bits/stdc++.h>
#include<conio.h>
using namespace std;
int main(){
    int num;
    cout<<"Enter a number : ";
    cin>>num;
    if(num>0){
        cout<<"positive number";
    }else if(num<0){
        cout<<"Negative number";
    }else if(num==0){
        cout<<"Zero";
    }
    getch();
}
